{
  "name": "alaycock.co.uk",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css:prod && npm run build:remix",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "build:remix": "remix build",
    "build:set-time": "node ./scripts/build-time.mjs",
    "build:info": "node ./scripts/generate-build-info.mjs",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css --watch",
    "dev:remix": "dotenv -- remix dev",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "install:fonts": "node ./scripts/fonts.mjs",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@arcath/utils": "^3.1.0",
    "@fontsource/montserrat": "^4.5.7",
    "@netlify/functions": "^1.0.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "@remix-run/server-runtime": "^1.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "canvas": "^2.9.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "esbuild": "^0.14.36",
    "framer-motion": "^6.3.0",
    "graphql-request": "^4.2.0",
    "mdx-bundler": "^9.0.0",
    "node-fetch": "^2.6.7",
    "octokit": "^1.7.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "prism-react-renderer": "^1.3.1",
    "promisify": "0.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.0.24",
    "twitter-api-v2": "^1.12.0",
    "unist-util-visit": "^4.1.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.0",
    "@types/mkdirp": "^1.0.2",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "copy-dir": "^1.3.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.13.0",
    "eslint-config-kentcdodds": "^20.2.0",
    "mkdirp": "^1.0.4",
    "postcss-cli": "^9.1.0",
    "typescript": "^4.6.3",
    "woff-tools": "^0.1.0"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*",
    "remix.env.d.ts"
  ]
}
