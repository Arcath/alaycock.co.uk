{
  "private": true,
  "name": "alaycock.co.uk",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css:prod && npm run build:remix",
    "build:remix": "remix build",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "build:css:prod": "npm run build:css -- --env production",
    "build:content": "ts-node ./scripts/posts.ts",
    "dev:remix": "dotenv -- remix dev",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css --watch",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "install:fonts": "ts-node-transpile-only ./scripts/fonts.ts"
  },
  "dependencies": {
    "@arcath/utils": "^1.10.0",
    "@fontsource/montserrat": "^4.5.5",
    "@netlify/functions": "^1.0.0",
    "@remix-run/react": "^1.3.2",
    "@remix-run/serve": "^1.3.2",
    "@remix-run/server-runtime": "^1.3.2",
    "@tailwindcss/typography": "^0.5.2",
    "canvas": "^2.9.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "esbuild": "^0.14.27",
    "fontsource-montserrat": "^4.0.0",
    "framer-motion": "^6.2.8",
    "graphql-request": "^4.1.0",
    "mdx-bundler": "^8.0.1",
    "octokit": "^1.7.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.0.2",
    "prism-react-renderer": "^1.3.1",
    "promisify": "0.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-gfm": "^3.0.1",
    "remix": "^1.3.2",
    "tailwindcss": "^3.0.23",
    "twitter-api-v2": "^1.11.1",
    "unist-util-visit": "^4.1.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.2",
    "@types/mkdirp": "^1.0.2",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "copy-dir": "^1.3.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "mkdirp": "^1.0.4",
    "msw": "^0.39.2",
    "postcss-cli": "^9.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
